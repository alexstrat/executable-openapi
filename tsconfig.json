{
  "compilerOptions": {
    "declaration": true,
    "strictNullChecks": true,
    "target": "es2016",
    "outDir": "dist",
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2015",
      "dom"
    ],
  },
}
